<!-- Modal -->
<div class="modal fade" id="classUpModal" data-bs-backdrop="static" data-bs-keyboard="false" tabindex="-1" aria-labelledby="classUpModalLabel" aria-hidden="true">
  <div class="modal-dialog modal-dialog-centered">
    <div class="modal-content">
      <div class="modal-header">
        <h1 class="modal-title fs-5" id="classUpModalLabel">เปลี่ยนชั้น</h1>
        <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
      </div>
      <div class="modal-body text-start">
        <label for="change_class" class="form-label">ต้องการเปลี่ยนชั้นเป็น</label>
        <input id="change_class" class="form-control" placeholder="1/1" />
      </div>
      <div class="modal-footer">
        <button type="button" class="btn btn-primary" data-action="click->user#class_up" id="confirm-classup">เปลี่ยน</button>
        <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">ยกเลิก</button>
      </div>
    </div>
  </div>
</div>